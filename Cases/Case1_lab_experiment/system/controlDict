FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      controlDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

application     HydroSedFoamkE;

startFrom       startTime;

startTime       900;

stopAt          endTime;

endTime         1200;

maxCo           0.3; // Or other Courant number you wish

adjustTimeStep  no; // yes Or no

maxDeltaT       0.1; // Maximum deltaT in seconds

deltaT          0.01;

writeControl    adjustableRunTime;

writeInterval   60;

purgeWrite      0;

writeFormat     ascii;

writePrecision  6;

writeCompression off;

timeFormat      general;

timePrecision   6;

runTimeModifiable false;

