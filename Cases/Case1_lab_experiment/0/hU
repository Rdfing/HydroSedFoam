FoamFile
{
    version     2.0;
    format      ascii;
    class       volVectorField;
    object      hU;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -1 0 0 0 0];

internalField   uniform (0 0 0);

boundaryField
{
    inlet
    {
        type            fixedValue;
        value           uniform (-0.0495 0 0);
    }
    sides
    {
        type            slip;
    }
    outlet
    {
        type            zeroGradient;
    }
    frontandback
    {
        type            empty;
    }
    frontandback
    {
        type            empty;
    }
}

